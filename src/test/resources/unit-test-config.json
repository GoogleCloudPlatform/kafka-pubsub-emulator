{
  "server": {
    "port": 8080,
    "security": {
      "certificateChainFile": "/path/to/server.crt",
      "privateKeyFile": "/path/to/server.key"
    }
  },
  "kafka": {
    "bootstrapServers": ["server1:2192", "server2:2192"],
    "producerProperties": {
      "linger.ms": "5",
      "batch.size": "1000000",
      "buffer.memory": "32000000"
    },
    "producerExecutors": 4,
    "consumerProperties": {
      "max.poll.records": "1000"
    },
    "consumersPerSubscription": 4
  },
  "pubsub": {
    "projects": [{
      "name": "project-1",
      "topics": [{
        "name": "topic-1",
        "kafkaTopic": "kafka-topic-1",
        "subscriptions": [{
          "name": "subscription-1",
          "ackDeadlineSeconds": 10
        }, {
          "name": "subscription-2",
          "ackDeadlineSeconds": 10
        }]
      }, {
        "name": "topic-2",
        "kafkaTopic": "kafka-topic-2",
        "subscriptions": [{
          "name": "subscription-3",
          "ackDeadlineSeconds": 30
        }, {
          "name": "subscription-4",
          "ackDeadlineSeconds": 45
        }]
      }]
    }, {
      "name": "project-2",
      "topics": [{
        "name": "topic-1",
        "kafkaTopic": "kafka-topic-1",
        "subscriptions": [{
          "name": "subscription-1",
          "ackDeadlineSeconds": 10
        }, {
          "name": "subscription-2",
          "ackDeadlineSeconds": 10
        }]
      }, {
        "name": "topic-2",
        "kafkaTopic": "kafka-topic-2",
        "subscriptions": [{
          "name": "subscription-3",
          "ackDeadlineSeconds": 30
        }]
      }]
    }]
  }
}